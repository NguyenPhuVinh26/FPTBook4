@using WebSiteBanSach4.Models;
@model List<ChuDe>
@{
    ViewBag.Title = "ThemeCategory";
    Layout = "~/Views/Layout/NguoiDungLayout.cshtml";
}

@foreach (var item in Model)
{
    <a id="btnChuDe" style="width:250px; font-size:11pt;color: aliceblue;background-color:#808080;text-decoration:none" href="@Url.Action("BookCategory", new {@MaChuDe=item.MaChuDe})" class="button button-submit">@item.TenChuDe</a>
    <br />
    <br />
}




